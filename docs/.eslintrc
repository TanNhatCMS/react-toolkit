{
  "extends": "../.eslintrc",
  "parser": "@typescript-eslint/parser",
  "overrides": [
    {
      "parserOptions": {
        "ecmaVersion": 10,
        "ecmaFeatures": {
          "jsx": true
        },
        "project": ["./tsconfig.json"]
      },
      "files": ["**/*.ts?(x)", "**/*.js?(x)"],
      "rules": {
        "react/react-in-jsx-scope": "off",
        "no-shadow": "off"
      }
    }
  ]
}
